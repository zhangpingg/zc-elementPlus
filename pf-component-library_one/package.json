{
  "name": "pf-component-library",
  "version": "0.3.2",
  "autor": "",
  "description": "pf-component-library组件库",
  "main": "dist/index.es.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "keywords": [
    "pf-component-library",
    "vue",
    "ui",
    "pf-ui",
    "pf-component-library组件库"
  ],
  "files": [
    "dist",
    "components"
  ],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --outDir dist/es --project tsconfig.build.json && vue-tsc --outDir dist/lib --project tsconfig.build.json && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "commit": "cz",
    "test": "vitest --run --passWithNoTests",
    "test:cov": "npm run test -- --coverage",
    "test:e2e": "playwright test",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "deploy": "bash docs/deploy.sh"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5",
    "axios": "^1.3.4",
    "element-plus": "^2.2.18",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "vue": "^3.4.27",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@playwright/test": "^1.44.1",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/glob": "^8.1.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^20.12.5",
    "@types/vue": "^2.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitepress-demo-preview/component": "^2.3.0",
    "@vitepress-demo-preview/plugin": "^1.1.9",
    "@vitest/coverage-v8": "^0.32.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/server-renderer": "^3.4.27",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "commitizen": "^4.3.0",
    "consola": "^3.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^8.0.0",
    "jsdom": "^21.1.0",
    "npm-run-all2": "^6.1.2",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.1",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-static-copy": "^1.0.5",
    "vite-plugin-style-import": "^2.0.0",
    "vite-svg-loader": "^5.1.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitepress": "^1.0.0-alpha.65",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.0.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}